{
  "secret_token_patterns": [
    {
      "name": "GitHub PAT (classic)",
      "pattern": "ghp_[A-Za-z0-9]{36}",
      "min_unique_chars": 0
    },
    {
      "name": "GitHub PAT (fine-grained)",
      "pattern": "github_pat_[A-Za-z0-9]{22}_[A-Za-z0-9]{59}",
      "min_unique_chars": 0
    },
    {
      "name": "GitHub OAuth",
      "pattern": "gho_[A-Za-z0-9]{36}",
      "min_unique_chars": 0
    },
    {
      "name": "GitHub App token",
      "pattern": "ghu_[A-Za-z0-9]{36}",
      "min_unique_chars": 0
    },
    {
      "name": "GitHub App refresh",
      "pattern": "ghr_[A-Za-z0-9]{36}",
      "min_unique_chars": 0
    },
    {
      "name": "Supabase service key",
      "pattern": "sbp_[A-Za-z0-9]{40}",
      "min_unique_chars": 0
    },
    {
      "name": "Anthropic API key",
      "pattern": "sk-ant-[A-Za-z0-9_-]{90,}",
      "min_unique_chars": 0
    },
    {
      "name": "OpenAI API key",
      "pattern": "sk-[A-Za-z0-9]{48}",
      "min_unique_chars": 8
    },
    {
      "name": "AWS access key",
      "pattern": "AKIA[A-Z0-9]{16}",
      "min_unique_chars": 0
    },
    {
      "name": "Stripe secret key",
      "pattern": "sk_(test|live)_[A-Za-z0-9]{24,}",
      "min_unique_chars": 0
    },
    {
      "name": "Stripe publishable key",
      "pattern": "pk_(test|live)_[A-Za-z0-9]{24,}",
      "min_unique_chars": 0
    },
    {
      "name": "Slack token",
      "pattern": "xox[bpras]-[A-Za-z0-9-]{10,}",
      "min_unique_chars": 0
    },
    {
      "name": "NPM token",
      "pattern": "npm_[A-Za-z0-9]{36}",
      "min_unique_chars": 0
    },
    {
      "name": "Generic JWT",
      "pattern": "eyJ[A-Za-z0-9_-]{20,}\\.eyJ[A-Za-z0-9_-]{20,}\\.[A-Za-z0-9_-]{20,}",
      "min_unique_chars": 10
    },
    {
      "name": "Generic high-entropy base64 (potential secret)",
      "pattern": "[A-Za-z0-9+/]{40,}={0,2}",
      "min_unique_chars": 15
    },
    {
      "name": "Private key block",
      "pattern": "-----BEGIN (RSA |EC |DSA |OPENSSH )?PRIVATE KEY-----",
      "min_unique_chars": 0
    },
    {
      "name": "Vercel token",
      "pattern": "vercel_[A-Za-z0-9]{24,}",
      "min_unique_chars": 0
    },
    {
      "name": "Netlify token",
      "pattern": "nfp_[A-Za-z0-9]{40}",
      "min_unique_chars": 0
    }
  ]
}
